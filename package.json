    "scripts": {
        "start": "set PORT=3002 && react-app-rewired start",
        "build": "set \"GENERATE_SOURCEMAP=false\" && react-scripts build",
        "build:qa": "sh -ac '. ./.env.qa; GENERATE_SOURCEMAP=false react-scripts build'",
        "build:stg": "sh -ac '. ./.env.stg; GENERATE_SOURCEMAP=false react-scripts build'",
        "build:prod": "sh -ac '. ./.env.prod; GENERATE_SOURCEMAP=false react-scripts build'",
        "test": "react-scripts test --coverage --watchAll=false --maxWorkers=4",
        "eject": "react-scripts eject",
        "ghp": "react-scripts build && gh-pages -d build"
    },
    "jest": {
        "collectCoverageFrom": [
